{
  "schemaVersion": 1,
  "id": "entitymodifiers",
  "version": "${version}",

  "name": "Entity Modifiers",
  "description": "A mod that adds entity modifiers to Minecraft.",
  "authors": [
    "ErrorCraft"
  ],
  "contact": {
    "homepage": "https://github.com/ErrorCraft/EntityModifiers",
    "sources": "https://github.com/ErrorCraft/EntityModifiers"
  },

  "license": "MIT",
  "icon": "assets/entitymodifiers/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "errorcraft.entitymodifiers.EntityModifiers"
    ]
  },
  "mixins": [
    "entitymodifiers.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
